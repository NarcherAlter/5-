#include<stdio.h>
#include<string.h>

int main()
{	
	int y = 0;
	int count = 0;
	for(y=1000;y>=1000 && y<=2000;y++)
	{	
		if(((y%4 == 0)&&(y%100 != 0))||(y%400 == 0))
			{printf("%d\n", y);
		count++;}
		/*if(y%4 == 0 && y%100 != 0)
			{printf("%d\n", y);
		count++;}
		else if(y%400 == 0)
			{printf("%d\n", y);
		count++;}*/
	}
	printf("\ncount = %d\n", count);
	return 0;
}
